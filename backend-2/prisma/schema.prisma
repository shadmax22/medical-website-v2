generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @default(uuid())
  name       String
  email_id   String   @unique
  phone_no   String
  dob        DateTime
  password   String
  stat       String
  role       String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("users")
}
